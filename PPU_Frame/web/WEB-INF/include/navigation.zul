			children="@load(vm.menuGroups)">
			
			<template name="children" var="menuGroup">
				<listbox style="margin-top:5px"
					ca:data-scrollable="false"
					width="260px"
					model="${menuGroup.menuItems}"
					onSelect='@command("showMessage", pos="end_center")'>
